{
  "extends": "google",
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "comma-dangle": ["error", "never"],
    "new-cap": ["error", {
      "capIsNew": false
    }],
    "eol-last": ["error", "never"]
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  }
}