{
  "extends": "google",
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "indent": ["error", 1],
    "comma-dangle": ["error", "never"],
    "new-cap": ["error", {"capIsNew": false}]
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  }
}
